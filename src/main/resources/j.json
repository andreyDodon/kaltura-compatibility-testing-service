{
  "type" : "object",
  "id" : "KalturaRequestConfiguration",
  "description" : "Define client request optional configurations",
  "dependencies" : {
    "KalturaBaseResponseProfile" : {
      "id" : "KalturaBaseResponseProfile",
      "description" : "Define base profile response -  optional configurations"
    },
    "KalturaSkipCondition" : {
      "id" : "KalturaSkipCondition",
      "description" : "Skip current request according to skip condition"
    }
  },
  "properties" : {
    "abortOnError" : {
      "type" : "boolean",
      "readonly" : false,
      "description" : "Abort the Multireuqset call if any error occurs in one of the requests"
    },
    "responseProfile" : {
      "type" : "object",
      "$ref" : "KalturaBaseResponseProfile",
      "readonly" : false,
      "description" : "Kaltura response profile object"
    },
    "abortAllOnError" : {
      "type" : "boolean",
      "readonly" : false,
      "description" : "Abort all following requests in Multireuqset if current request has an error"
    },
    "ks" : {
      "type" : "string",
      "readonly" : false,
      "description" : "Kaltura API session"
    },
    "language" : {
      "type" : "string",
      "readonly" : false,
      "description" : "Content language"
    },
    "currency" : {
      "type" : "string",
      "readonly" : false,
      "description" : "Currency to be used"
    },
    "skipCondition" : {
      "type" : "object",
      "$ref" : "KalturaSkipCondition",
      "readonly" : false,
      "description" : "Skip current request according to skip condition"
    },
    "partnerId" : {
      "type" : "integer",
      "readonly" : false,
      "description" : "Impersonated partner id"
    },
    "userId" : {
      "type" : "integer",
      "readonly" : false,
      "description" : "Impersonated user id"
    }
  }
}